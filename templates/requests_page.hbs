        <td id="phone_number"> - </td>
      </tr>
    </table>
    <div class="repairs_details_box_inner" >
    	<button id="print-shipping-label" class="button-white button" target="_blank">{{dc 'repairs_details_header_shippinglabel_tooltip'}}</button>
    	<div id="shipping_label" class=""></div>
    </div> 
        </div>
